<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.match_scouting.MatchScoutingFragment">
    <TextView
        android:id="@+id/text_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="469dp"
                android:layout_height="254dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="@string/auto" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/auto_amp_dec"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/dec_button" />
                    <TextView
                        android:id="@+id/amp_note_auto_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amp_notes"
                        />
                    <TextView
                        android:id="@+id/amp_note_auto"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                         />
                    <Button
                        android:id="@+id/auto_amp_inc"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/auto_speaker_dec"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/speaker_note_auto_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/speaker_notes" />
                    <TextView
                        android:id="@+id/speaker_note_auto"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        />
                    <Button
                        android:id="@+id/auto_speaker_inc"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/leave"
                    android:layout_width="match_parent"
                    android:layout_height="71dp"
                    android:text="@string/leave" />

                <TextView
                    android:id="@+id/endgame_human_player"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="@string/endgame_human_player" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="542dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tele_op"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="@string/tele_op" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/tele_op_amp_dec"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_note_tele_op_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amp_notes"
                        />
                    <TextView
                        android:id="@+id/amp_note_tele_op"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        />

                    <Button
                        android:id="@+id/tele_op_amp_inc"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/inc_button" />

                    <Space
                        android:layout_width="96dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <Button
                        android:id="@+id/submit"
                        android:layout_width="130dp"
                        android:layout_height="wrap_content"
                        android:text="@string/submit" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/tele_op_speaker_dec"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/dec_button" />
                    <TextView
                        android:id="@+id/tele_op_speaker_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/speaker_notes"
                        />
                    <TextView
                        android:id="@+id/tele_op_speaker"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        />
                    <Button
                        android:id="@+id/tele_op_speaker_inc"
                        android:layout_height="wrap_content"
                        android:layout_width="75dp"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/amp_speaker_note_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_speaker_note_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amplified_speaker_notes" />

                    <TextView
                        android:id="@+id/amp_speaker_note"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        />

                    <Button
                        android:id="@+id/amp_speaker_note_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/on_stage"
                        android:layout_width="194dp"
                        android:layout_height="wrap_content"
                        android:text="@string/on_stage" />

                    <CheckBox
                        android:id="@+id/on_stage_spotlit"
                        android:layout_width="193dp"
                        android:layout_height="wrap_content"
                        android:text="@string/on_stage_spotlit" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/trap_note"
                        android:layout_width="193dp"
                        android:layout_height="wrap_content"
                        android:text="@string/trap_note" />

                    <TextView
                        android:id="@+id/spotlit_desc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/high_ring_on_pole_over_chain" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="469dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/amps_dec"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:text="@string/dec_button" />

            <TextView
                android:id="@+id/amps_text"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="@string/amplifiers" />

            <TextView
                android:id="@+id/amps"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                />

            <Button
                android:id="@+id/amps_inc"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:text="@string/inc_button" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="87dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/co_op"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/coopertition"
                    tools:ignore="NestedWeights" />

                <CheckBox
                    android:id="@+id/park"
                    android:layout_width="135dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/parked" />

                <CheckBox
                    android:id="@+id/melody"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/melody" />

                <CheckBox
                    android:id="@+id/ensamble"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/ensamble" />

                <CheckBox
                    android:id="@+id/harmony"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/harmony" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Space
                    android:layout_width="346dp"
                    android:layout_height="26dp"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights" />

                <TextView
                    android:id="@+id/melody_desc"
                    android:layout_width="72dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/_18_15_co_op_notes" />

                <TextView
                    android:id="@+id/ensamble_desc"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/_10_stage_points_2_onstage_robot" />

                <TextView
                    android:id="@+id/harmony_desc"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/_2_robot_same_chain" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

