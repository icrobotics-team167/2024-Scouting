<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.match_scouting.MatchScoutingFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <Space
            android:layout_width="658dp"
            android:layout_height="38dp" />

        <LinearLayout
            android:layout_width="659dp"
            android:layout_height="276dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="330dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/auto"
                        android:layout_width="39dp"
                        android:layout_height="24dp"
                        android:ems="10"
                        android:text="@string/auto" />

                    <CheckBox
                        android:id="@+id/leave"
                        android:layout_width="79dp"
                        android:layout_height="match_parent"
                        android:text="@string/leave" />

                    <TextView
                        android:id="@+id/rings"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rings:" />

                    <LinearLayout
                        android:layout_width="170dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <CheckBox
                            android:id="@+id/ring1"
                            android:layout_width="36dp"
                            android:layout_height="match_parent"
                            android:text="" />

                        <CheckBox
                            android:id="@+id/ring2"
                            android:layout_width="31dp"
                            android:layout_height="wrap_content"
                            android:text="" />

                        <CheckBox
                            android:id="@+id/ring3"
                            android:layout_width="4dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="" />

                        <CheckBox
                            android:id="@+id/ring4"
                            android:layout_width="2dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="" />

                        <CheckBox
                            android:id="@+id/ring5"
                            android:layout_width="3dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/auto_amp_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_note_auto_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amp_notes" />

                    <TextView
                        android:id="@+id/amp_note_auto"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/auto_amp_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/auto_speaker_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/speaker_note_auto_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/speaker_notes" />

                    <TextView
                        android:id="@+id/speaker_note_auto"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/auto_speaker_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="148dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/endgame_human_player"
                                android:layout_width="101dp"
                                android:layout_height="wrap_content"
                                android:ems="10"
                                android:text="@string/endgame_human_player" />

                            <CheckBox
                                android:id="@+id/park"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_weight="1"
                                android:text="@string/parked" />

                            <CheckBox
                                android:id="@+id/defence"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:layout_weight="1"
                                android:text="Defence" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/field"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:src="@drawable/screenshot_2024_02_18_at_3_48_17pm" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tele_op"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:text="@string/tele_op" />

                    <Button
                        android:id="@+id/submit"
                        android:layout_width="169dp"
                        android:layout_height="31dp"
                        android:textSize="10dp"
                        android:text="@string/submit" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="329dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/tele_op_amp_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_note_tele_op_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amp_notes" />

                    <TextView
                        android:id="@+id/amp_note_tele_op"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/tele_op_amp_inc"
                        android:layout_width="77dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />

                    <Space
                        android:layout_width="96dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="330dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/tele_op_speaker_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/tele_op_speaker_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/speaker_notes" />

                    <TextView
                        android:id="@+id/tele_op_speaker"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/tele_op_speaker_inc"
                        android:layout_width="78dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/amp_speaker_note_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_speaker_note_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amplified_speaker_notes" />

                    <TextView
                        android:id="@+id/amp_speaker_note"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/amp_speaker_note_inc"
                        android:layout_width="78dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="330dp"
                    android:layout_height="21dp"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/on_stage"
                        android:layout_width="153dp"
                        android:layout_height="21dp"
                        android:text="@string/on_stage" />

                    <CheckBox
                        android:id="@+id/on_stage_spotlit"
                        android:layout_width="176dp"
                        android:layout_height="match_parent"
                        android:text="@string/on_stage_spotlit" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="330dp"
                    android:layout_height="21dp"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/trap_note"
                        android:layout_width="151dp"
                        android:layout_height="match_parent"
                        android:text="@string/trap_note" />

                    <TextView
                        android:id="@+id/spotlit_desc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/high_ring_on_pole_over_chain" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/shooting_distance"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Shooting distance:  SP    IW    OW    PO" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <Space
                                android:layout_width="27dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1" />

                            <SeekBar
                                android:id="@+id/shooting_distance_bar"
                                style="@style/Widget.AppCompat.SeekBar.Discrete"
                                android:layout_width="62dp"
                                android:layout_height="37dp"
                                android:layout_weight="1"
                                android:max="3"
                                android:progress="0" />

                            <Space
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

