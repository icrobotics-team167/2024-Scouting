<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.match_scouting.MatchScoutingFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <Space
            android:layout_width="658dp"
            android:layout_height="38dp" />

        <LinearLayout
            android:layout_width="660dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="330dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/auto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="@string/auto" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/auto_amp_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_note_auto_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amp_notes" />

                    <TextView
                        android:id="@+id/amp_note_auto"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/auto_amp_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/auto_speaker_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/speaker_note_auto_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/speaker_notes" />

                    <TextView
                        android:id="@+id/speaker_note_auto"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/auto_speaker_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/leave"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/leave" />

                <TextView
                    android:id="@+id/endgame_human_player"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="@string/endgame_human_player" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="74dp"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/park"
                        android:layout_width="184dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="@string/parked" />

                    <Button
                        android:id="@+id/submit"
                        android:layout_width="130dp"
                        android:layout_height="74dp"
                        android:text="@string/submit" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tele_op"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="@string/tele_op" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/tele_op_amp_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_note_tele_op_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amp_notes" />

                    <TextView
                        android:id="@+id/amp_note_tele_op"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/tele_op_amp_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />

                    <Space
                        android:layout_width="96dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/tele_op_speaker_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/tele_op_speaker_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/speaker_notes" />

                    <TextView
                        android:id="@+id/tele_op_speaker"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/tele_op_speaker_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/amp_speaker_note_dec"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/dec_button" />

                    <TextView
                        android:id="@+id/amp_speaker_note_text"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/amplified_speaker_notes" />

                    <TextView
                        android:id="@+id/amp_speaker_note"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content" />

                    <Button
                        android:id="@+id/amp_speaker_note_inc"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:text="@string/inc_button" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/on_stage"
                        android:layout_width="153dp"
                        android:layout_height="wrap_content"
                        android:text="@string/on_stage" />

                    <CheckBox
                        android:id="@+id/on_stage_spotlit"
                        android:layout_width="173dp"
                        android:layout_height="wrap_content"
                        android:text="@string/on_stage_spotlit" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/trap_note"
                        android:layout_width="151dp"
                        android:layout_height="wrap_content"
                        android:text="@string/trap_note" />

                    <TextView
                        android:id="@+id/spotlit_desc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/high_ring_on_pole_over_chain" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

